<footer>
  <div class="wide_links">
    <div class="row">
      <div class='col-sm-4'>
        <span style="color: #888; padding-right: 10px;">{{t "CoughDrop" key='coughdrop'}}</span>
        {{#unless app_state.no_linky}}
          {{#if app_state.installed_app}}
            <a href="https://www.mycoughdrop.com" target="_blank">{{t "Web Site" key='web_site'}}</a> <span class='text-muted'>|</span>
          {{/if}}
          {{#link-to "about"}}{{t "About" key='about'}}{{/link-to}} <span class='text-muted'>|</span>
          {{#unless app_state.installed_app}}
            {{#if app_state.feature_flags.subscriptions}}
              {{#link-to "pricing"}}{{t "Pricing" key='about'}}{{/link-to}} <span class='text-muted'>|</span>
            {{/if}}
          {{/unless}}
        {{/unless}}
        <a href="#" {{action 'support'}}>{{t "Support" key='support'}}</a>
        {{#unless app_state.installed_app}}
          <span class='text-muted'>|</span>
          {{#link-to "contact"}}{{t "Contact Us" key='contact'}}{{/link-to}}
        {{/unless}}
      </div>
      <div class='col-sm-3 col-sm-offset-3' style="text-align: right;">
        {{#unless app_state.installed_app}}
          <a href="https://github.com/CoughDrop" target="_blank">{{t "Developers" key='developers'}}</a> <span class='text-muted'>|</span>
          {{#if persistence.online}}
            {{#link-to "jobs"}}{{t "Jobs" key='jobs'}}{{/link-to}} <span class='text-muted'>|</span>
          {{/if}}
        {{/unless}}
        {{#link-to "privacy"}}{{t "Privacy" key='privacy'}}{{/link-to}} <span class='text-muted'>|</span>
        {{#link-to "terms"}}{{t "Terms" key='terms'}}{{/link-to}}
      </div>
      {{#if persistence.online}}
        {{#unless app_state.no_linky}}
          <div class='col-sm-2'>
            <!-- icons found here: https://www.iconfinder.com/icons/104443/feed_rss_subscribe_icon#size=24 -->
            <span class="icons">
              <a href="http://blog.mycoughdrop.com" target="_blank"><img src="data:image/png;base64," alt="{{t "Blog" key='blog'}}"/></a>
              <a href="https://twitter.com/CoughDropAAC" target="_blank"><img src="data:image/png;base64," alt="{{t "Twitter" key='twitter'}}"/></a>
              <a href="https://www.facebook.com/CoughDropAAC" target="_blank"><img src="data:image/png;base64," alt="{{t "Facebook" key='facebook'}}"/></a>
              <a href="https://plus.google.com/+myCoughDropAAC" rel="publisher" target="_blank"><img src="data:image/png;base64," alt="{{t "Google+" key="google_plus"}}"/></a>
            </span>
          </div>
        {{/unless}}
      {{/if}}
    </div>
  </div>
  {{#if app_state.index_view}}
  <div class="skinny_links">
    <span class="dropdown">
      <span class="dropup">
      <a data-toggle="dropdown" href="#">{{t "More Resources" key="more_resources"}} <span class='caret'></span></a>
      <ul class="dropdown-menu dropdown-menu-top top" role="menu" aria-labelledby="dLabel">
        {{#unless app_state.no_linky}}
          <li>
            <a href="https://www.mycoughdrop.com" target="_blank">{{t "Web Site" key='web_site'}}</a>
          </li>
          <li>
            {{#link-to "about"}}{{t "About" key='about'}}{{/link-to}}
          </li>
        {{/unless}}
        <li>
          <a href="#" {{action 'support'}}>{{t "Support" key='support'}}</a>
        </li>
        <li>
          {{#link-to "privacy"}}{{t "Privacy" key='privacy'}}{{/link-to}}
        </li>
        <li>
          {{#link-to "terms"}}{{t "Terms" key='terms'}}{{/link-to}}
        </li>
        {{#unless app_state.no_linky}}
          {{#if persistence.online}}
            <li>
              <!-- icons found here: https://www.iconfinder.com/icons/104443/feed_rss_subscribe_icon#size=24 -->
              <span class="icons" style="padding-left: 17px;">
                <a href="http://blog.mycoughdrop.com" target="_blank"><img src="data:image/png;base64," alt="{{t "Blog" key='blog'}}"/></a>
                <a href="https://twitter.com/CoughDropAAC" target="_blank"><img src="data:image/png;base64," alt="{{t "Twitter" key='twitter'}}"/></a>
                <a href="https://www.facebook.com/CoughDropAAC" target="_blank"><img src="data:image/png;base64," alt="{{t "Facebook" key='facebook'}}"/></a>
                <a href="https://plus.google.com/+myCoughDropAAC" rel="publisher" target="_blank"><img src="data:image/png;base64," alt="{{t "Google+" key="google_plus"}}"/></a>
              </span>
            </li>
          {{/if}}
        {{/unless}}
      </ul>
      </span>
    </span>
  </div>
  {{/if}}
</footer>
